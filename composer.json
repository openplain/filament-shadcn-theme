{
    "name": "openplain/filament-shadcn-theme",
    "description": "Shadcn UI theme for Filament - Beautiful color themes with the iconic Shadcn design system",
    "keywords": [
        "filament",
        "filament-theme",
        "shadcn",
        "shadcn-ui",
        "dark-mode",
        "light-mode",
        "theme",
        "colors"
    ],
    "homepage": "https://github.com/openplain/filament-shadcn-theme",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Openplain",
            "email": "hello@openplain.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Openplain\\FilamentShadcnTheme\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Openplain\\FilamentShadcnTheme\\FilamentShadcnThemeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}